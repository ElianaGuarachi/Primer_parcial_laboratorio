
#ifndef CLIENTES_H_
#define CLIENTES_H_

#include <stdio.h>
#include <stdlib.h>
#include "utn.h"
#define LIBRE 0
#define OCUPADO 1

typedef struct
{
	int idCliente;
	char nombreEmpresa[51];
	char cuit[51];
	char direccion[51];
	char localidad[51];
	int isEmpty;
}eClientes;

int InicilizarClientes(eClientes clientes[], int cantidad);
int BuscarLibre(eClientes clientes[], int cantidad);
eClientes InformacionDeUnCliente();
int AltaDeCliente(eClientes clientes[], int cantidad, int idClientes);

#endif /* CLIENTES_H_ */
